Program.Sub.loadDTAllShip.Start
	F.Intrinsic.Control.Try
	V.Local.sError.Declare
	
	V.Local.sFilter.Declare
	V.Local.dCutDate.Declare

	Gui.frmShip.lbl1.Visible(True)
	Gui.frmShip.lbl1.ForeColor(V.Color.Black)
	Gui.frmShip.lbl1.Caption("Loading Shipments....")

	F.Intrinsic.Control.If(V.DataTable.dtAll.Exists,=,True)
		F.Data.DataTable.Close("dtAll")
	F.Intrinsic.Control.EndIf

	F.Intrinsic.Control.If(V.DataTable.dtAllShip.Exists,=,True)
		F.Data.DataTable.Close("dtAllShip")
	F.Intrinsic.Control.EndIf
	
	V.Global.bLoadData.Set(True)

	V.Local.sSQL.Declare(String,"Select Distinct CASE WHEN A.PICK_LIST_PRINTED = 'Y' THEN 'RePrint' ELSE 'Print' END AS PickLs,'Labels' AS LABELS,C.NAME_CUSTOMER,A.PRODUCT_LINE,COALESCE(A.PART,'') AS PART,'' As DISPLAY_PART,A.DESCRIPTION,A.ORDER_NO,LEFT(A.Record_NO,3) AS RECORD_NO,CASE WHEN A.FLAG_USE_MQD != 'Y' THEN A.QTY_BO WHEN FLAG_USE_MQD = 'Y' THEN B.QTY_OPEN END AS QTY_BO,H.WEIGHT AS WEIGHT_GAB,H.BOX_QTY,CASE WHEN A.FLAG_USE_MQD != 'Y' THEN A.DATE_ITEM_PROM WHEN FLAG_USE_MQD = 'Y' THEN IF(B.DATE_DUE = '1900-01-01',CONVERT('9999-12-31',SQL_DATE),B.DATE_DUE) END AS DATE_ITEM_PROM,(D.QTY_ONHAND-D.QTY_SAFETY) AS QTY_ONHAND,E.CITY_SHIP,E.CODE_ZIP_SHIP,E.SHIP_VIA_SHIP,CONVERT(A.STAGED,SQL_BIT) AS STAGED,CONVERT(IF(LEFT(A.USER_5,8) = 'Complete',1,0),SQL_BIT) AS COMPLETE,CASE WHEN FLAG_USE_MQD = 'Y' THEN 'Ship Schd' ELSE '-' END AS FLAG_USE_MQD,A.USER_5,G.NOTES,A.CUSTOMER,A.SHIP_ID,A.QTY_SHIPPED,CASE WHEN F.SHP_HLD_FLAG = 02 OR F.SHP_HLD_FLAG = 04 THEN CONVERT(1,SQL_BIT) WHEN F.SHP_HLD_FLAG = NULL THEN CONVERT(0,SQL_BIT) ELSE CONVERT(0,SQL_BIT) END AS SHP_HLD_FLAG,A.USER_4,A.LOCATION,A.WEIGHT,E.CARRIER_CD,'' AS COLOR FROM V_ORDER_LINES A LEFT JOIN V_OE_SHIP_SCHED B ON B.ORDER_NO = A.ORDER_NO AND B.ORDER_LINE = A.RECORD_NO LEFT JOIN V_CUSTOMER_MASTER C ON A.CUSTOMER = C.CUSTOMER LEFT JOIN V_INVENTORY_MSTR D ON A.PART = D.PART AND A.LOCATION = D.LOCATION LEFT JOIN V_ORDER_SHIP_TO E ON E.ORDER_NO = A.ORDER_NO AND E.SHIP_ID = A.SHIP_ID LEFT JOIN ORDER_HEADER F ON A.ORDER_NO = F.ORDER_NO LEFT JOIN GAB_4167_SO_NOTES G ON A.ORDER_NO = G.SO AND RECORD_NO = G.SOLINE LEFT JOIN GAB_INV_SD_INFO H ON A.PART = H.PART AND A.LOCATION = H.LOC WHERE F.RECORD_NO = '  A' AND QTY_BO <> 0 ORDER BY DATE_ITEM_PROM,A.ORDER_NO,RECORD_NO;")
	F.Data.DataTable.CreateFromSQL("dtAll","con",v.Local.sSQL,True)
	
	f.Intrinsic.Control.If(v.Global.bLongPart)
		F.Data.DataTable.AddDisplayPartColumn("dtAll",0,"PART","DISPLAY_PART")
	f.Intrinsic.Control.Else
		f.Data.DataTable.CopyColumn("dtAll","PART","DISPLAY_PART")
	f.Intrinsic.Control.EndIf	

	V.Local.dCutDate.Set(V.Screen.frmShip!dtpCutoff.Value)
	F.Intrinsic.String.Build("DATE_ITEM_PROM <= #{0}#",V.Local.dCutDate,V.Local.sFilter)
	F.Data.DataView.Create("dtAll","dvAllShip",22,V.Local.sFilter,"DATE_ITEM_PROM,ORDER_NO,RECORD_NO")
	F.Data.DataView.ToDataTable("dtAll","dvAllShip","dtAllShip",True)

	F.Intrinsic.Control.CallSub(CheckTopLevelReqs)
	F.Intrinsic.Control.CallSub(CheckLevel1Reqs)

	F.Intrinsic.Control.CallSub(loadDVGVAllShip)
	
	F.Intrinsic.Control.Catch
		F.Intrinsic.String.Build("Project: {0}{1}{1}Subroutine: {2}{1}Error Occurred {3} with description {4}{1}{1}GAB Version: {5}",V.Ambient.ScriptPath,V.Ambient.Newline,V.Ambient.CurrentSubroutine,V.Ambient.ErrorNumber,V.Ambient.ErrorDescription,V.Ambient.GABVersion,V.Local.sError)
		F.Intrinsic.UI.Msgbox(V.Local.sError)
		F.Intrinsic.Control.End 
	F.Intrinsic.Control.EndTry
Program.Sub.loadDTAllShip.End

