Program.Sub.CheckTopLevelReqs.Start
	F.Intrinsic.Control.Try
	V.Local.sError.Declare
	
	V.Local.iCnt.Declare
	V.Local.sColor.Declare
	V.Local.sPart.Declare
	V.Local.sLoc.Declare
	V.Local.sSQL.Declare
	V.Local.fQtyOnHand.Declare
	V.Local.fBOQty.Declare
	V.Local.fNewOHQty.Declare

	Gui.frmShip.progressBar1.Visible(True)
	Gui.frmShip.progressBar1.Min(0)
	Gui.frmShip.progressBar1.Max(V.DataTable.dtAllShip.RowCount)

	F.ODBC.Connection!con.Execute("DELETE FROM GAB_4192_PARTS;")

	F.Intrinsic.Control.For(V.Local.iCnt,0,V.DataTable.dtAllShip.RowCount--,1)
		Gui.frmShip.progressBar1.Value(V.Local.iCnt)
		V.Local.sColor.Set("")
		F.Intrinsic.Control.If(V.DataTable.dtAllShip(V.Local.iCnt).QTY_ONHAND!FieldValTrim,<>,"")
			F.Intrinsic.Control.If(V.DataTable.dtAllShip(V.Local.iCnt).SHP_HLD_FLAG!FieldVal,<>,True)
				V.Local.sPart.Set(V.DataTable.dtAllShip(V.Local.iCnt).PART!FieldValTrim)
				V.Local.sLoc.Set(V.DataTable.dtAllShip(V.Local.iCnt).LOCATION!FieldValTrim)
				F.Intrinsic.String.Build("SELECT * FROM GAB_4192_PARTS WHERE PART = '{0}';",V.Local.sPart,V.Local.sSQL)
				F.ODBC.Connection!con.OpenRecordsetRW("rstParts",V.Local.sSQL)
				F.Intrinsic.Control.If(V.ODBC.con!rstParts.EOF)
					V.Local.fQtyOnHand.Set(V.DataTable.dtAllShip(V.Local.iCnt).QTY_ONHAND!FieldValFloat)
					F.ODBC.con!rstParts.AddNew
					F.ODBC.con!rstParts.Set!PART(V.Local.sPart)
					F.ODBC.con!rstParts.Set!LOCATION("")
					F.ODBC.con!rstParts.Set!ORIG_ONHAND(V.Local.fQtyOnHand)
					F.ODBC.con!rstParts.Set!NEW_ONHAND(V.Local.fQtyOnHand)
					F.ODBC.con!rstParts.Update
					F.ODBC.con!rstParts.Requery
				F.Intrinsic.Control.Else
					V.Local.fQtyOnHand.Set(V.ODBC.con!rstParts.FieldValFloat!NEW_ONHAND)
					F.Data.DataTable.SetValue("dtAllShip",V.Local.iCnt,"QTY_ONHAND",V.Local.fQtyOnHand)
				F.Intrinsic.Control.EndIf

				V.Local.fBOQty.Set(V.DataTable.dtAllShip(V.Local.iCnt).QTY_BO!FieldValFloat)
				F.Intrinsic.Control.If(V.Local.fQtyOnHand,>=,V.Local.fBOQty)
					V.Local.sColor.Set("G")
					F.Intrinsic.Math.Sub(V.Local.fQtyOnHand,V.Local.fBOQty,V.Local.fNewOHQty)
					F.ODBC.con!rstParts.Set!NEW_ONHAND(V.Local.fNewOHQty)
					F.ODBC.con!rstParts.Update
					F.ODBC.con!rstParts.Close
				F.Intrinsic.Control.Else(V.Local.fQtyOnHand,<,V.Local.fBOQty)
					V.Local.sColor.Set("")
					F.Intrinsic.Math.Sub(V.Local.fQtyOnHand,V.Local.fBOQty,V.Local.fNewOHQty)
					F.ODBC.con!rstParts.Set!NEW_ONHAND(V.Local.fNewOHQty)
					F.ODBC.con!rstParts.Update
					F.ODBC.con!rstParts.Close
					F.Intrinsic.Control.If(V.Local.fNewOHQty,<=,0)
						F.Data.DataTable.SetValue("dtAllShip",V.Local.iCnt,"QTY_ONHAND",V.Local.fQtyOnHand)
						F.Intrinsic.Control.If(V.Local.fQtyOnHand,<=,0)
							F.Data.DataTable.SetValue("dtAllShip",V.Local.iCnt,"QTY_ONHAND",0)
						F.Intrinsic.Control.EndIf
					F.Intrinsic.Control.Else
						F.Data.DataTable.SetValue("dtAllShip",V.Local.iCnt,"QTY_ONHAND",V.Local.fNewOHQty)
					F.Intrinsic.Control.EndIf
				F.Intrinsic.Control.EndIf
			F.Intrinsic.Control.EndIf
		F.Intrinsic.Control.EndIf

		F.Intrinsic.Control.If(V.Local.sColor,<>,"")
			F.Data.DataTable.SetValue("dtAllShip",V.Local.iCnt,"COLOR",V.Local.sColor)
		F.Intrinsic.Control.EndIf
	F.Intrinsic.Control.Next(V.Local.iCnt)
	
	F.Intrinsic.Control.Catch
		F.Intrinsic.String.Build("Project: {0}{1}{1}Subroutine: {2}{1}Error Occurred {3} with description {4}{1}{1}GAB Version: {5}",V.Ambient.ScriptPath,V.Ambient.Newline,V.Ambient.CurrentSubroutine,V.Ambient.ErrorNumber,V.Ambient.ErrorDescription,V.Ambient.GABVersion,V.Local.sError)
		F.Intrinsic.UI.Msgbox(V.Local.sError)
		F.Intrinsic.Control.End 
	F.Intrinsic.Control.EndTry
Program.Sub.CheckTopLevelReqs.End

Program.Sub.CheckLevel1Reqs.Start
	F.Intrinsic.Control.Try
	V.Local.sError.Declare
	
	V.Local.iCnt.Declare
	V.Local.sParent.Declare
	V.Local.sPart.Declare
	V.Local.sLoc.Declare
	V.Local.sSQL.Declare
	V.Local.sColor.Declare
	V.Local.dStartDate.Declare
	V.Local.fQtyCommit.Declare
	V.Local.fQtyOnHand.Declare
	V.Local.fQtyReqd.Declare
	V.Local.fNewReqQty.Declare
	V.Local.fQtyDiff.Declare
	V.Local.fBOMQty.Declare
	V.Local.fNewBOMQtyReqd.Declare
	V.Local.fNewOHQty.Declare

	Gui.frmShip.progressBar1.Visible(True)
	Gui.frmShip.progressBar1.Min(0)
	Gui.frmShip.progressBar1.Max(V.DataTable.dtAllShip.RowCount)

	F.Intrinsic.Control.For(V.Local.iCnt,0,V.DataTable.dtAllShip.RowCount--,1)
		Gui.frmShip.progressBar1.Value(V.Local.iCnt)
		V.Local.sColor.Set("")
		F.Intrinsic.Control.If(V.DataTable.dtAllShip(V.Local.iCnt).QTY_ONHAND!FieldValTrim,<>,"")
			F.Intrinsic.Control.If(V.DataTable.dtAllShip(V.Local.iCnt).QTY_BO!FieldValFloat,>,V.DataTable.dtAllShip(V.Local.iCnt).QTY_ONHAND!FieldValFloat)
				V.Local.sParent.Set(V.DataTable.dtAllShip(V.Local.iCnt).PART!FieldValTrim)
				V.Local.sLoc.Set(V.DataTable.dtAllShip(V.Local.iCnt).LOCATION!FieldValTrim)
				F.Intrinsic.String.Build("SELECT A.PARENT,A.SEQUENCE_BOM,A.QUANTITY,A.PART,IF(B.QTY_ONHAND IS NULL,0,B.QTY_ONHAND) QTY_ONHAND,IF(B.QTY_REQUIRED IS NULL,0,B.QTY_REQUIRED) QTY_REQUIRED FROM V_BOM_MSTR A LEFT JOIN V_INVENTORY_MSTR B ON A.PART = B.PART WHERE PARENT = '{0}' AND LOCATION = '' AND SEQUENCE_BOM <> '0000' AND A.PART NOT LIKE 'NT%' AND A.PART NOT LIKE 'WA%' AND A.PART NOT LIKE 'CA%' AND A.PART NOT LIKE 'MNT%' AND A.PART NOT LIKE 'WC%' AND A.PART NOT LIKE 'PN%' ORDER BY SEQUENCE_BOM;",V.Local.sParent,V.Local.sSQL)
				F.ODBC.Connection!con.OpenRecordsetRO("rstBOM",V.Local.sSQL)
				F.Intrinsic.Control.DoUntil(V.ODBC.con!rstBOM.EOF,=,True)
					V.Local.sPart.Set(V.ODBC.con!rstBOM.FieldValLTrim!PART)
					F.Intrinsic.String.Build("SELECT A.PARENT,A.SEQUENCE_BOM,A.QUANTITY,A.PART,IF(B.QTY_ONHAND IS NULL,0,B.QTY_ONHAND) QTY_ONHAND,IF(B.QTY_REQUIRED IS NULL,0,B.QTY_REQUIRED) QTY_REQUIRED FROM V_BOM_MSTR A LEFT JOIN V_INVENTORY_MSTR B ON A.PART = B.PART WHERE PARENT = '{0}' AND A.PART = '{1}' AND LOCATION = '';",V.Local.sParent,V.Local.sPart,V.Local.sSQL)
					F.ODBC.Connection!con.OpenRecordsetRO("rstBOM2",V.Local.sSQL)
					V.Local.fBOMQty.Set(V.ODBC.con!rstBOM2.FieldValFloat!QUANTITY)
					V.Local.fQtyOnHand.Set(V.ODBC.con!rstBOM2.FieldValFloat!QTY_ONHAND)
					V.Local.fQtyReqd.Set(V.ODBC.con!rstBOM2.FieldValFloat!QTY_REQUIRED)
					F.ODBC.con!rstBOM2.Close
					F.Intrinsic.Control.If(V.Local.fQtyOnHand,>=,V.Local.fQtyReqd)
					F.Intrinsic.Control.AndIf(V.Local.fQtyReqd,>,0)
						V.Local.sColor.Set("Y")
					F.Intrinsic.Control.ElseIf(V.Local.fQtyOnHand,<,V.Local.fQtyReqd)
						V.Local.dStartDate.Set(V.Screen.frmShip!dtpCutoff.Value)
						F.Intrinsic.String.Build("SELECT SUM(QTY_COMMITTED) FROM JOB_COMMITMENTS WHERE IF (SUBSTRING (JOB_COMMITMENTS.START_DATE ,5 ,2 )<= '99' AND SUBSTRING (JOB_COMMITMENTS.START_DATE ,5 ,2 )>= '00' AND SUBSTRING (JOB_COMMITMENTS.START_DATE ,1 ,2 )<= '12' AND SUBSTRING (JOB_COMMITMENTS.START_DATE ,1 ,2 )>= '01' AND SUBSTRING (JOB_COMMITMENTS.START_DATE ,3 ,2 )<= '31' AND SUBSTRING (JOB_COMMITMENTS.START_DATE ,3 ,2 )>= '01' ,IF (SUBSTRING (JOB_COMMITMENTS.START_DATE ,5 ,2 )< '80' ,CONVERT ('20' +SUBSTRING (JOB_COMMITMENTS.START_DATE ,5 ,2 )+'-' +SUBSTRING (JOB_COMMITMENTS.START_DATE ,1 ,2 )+'-' +SUBSTRING (JOB_COMMITMENTS.START_DATE ,3 ,2 ),SQL_DATE),CONVERT('19' +SUBSTRING (JOB_COMMITMENTS.START_DATE ,5 ,2 )+'-' +SUBSTRING (JOB_COMMITMENTS.START_DATE ,1 ,2 )+'-' +SUBSTRING (JOB_COMMITMENTS.START_DATE ,3 ,2 ),SQL_DATE)),CONVERT('1900-01-01' ,SQL_DATE)) > '{0}' AND PART = '{1}';",V.Local.dStartDate.PervasiveDate,V.Local.sPart,V.Local.sSQL)
						F.ODBC.Connection!con.ExecuteAndReturn(V.Local.sSQL,V.Local.fQtyCommit)
						F.Intrinsic.Math.Sub(V.Local.fQtyReqd,V.Local.fQtyCommit,V.Local.fNewReqQty)
						F.Intrinsic.Control.If(V.Local.fNewReqQty,<=,V.Local.fQtyOnHand)
							V.Local.sColor.Set("Y")
						F.Intrinsic.Control.ElseIf(V.Local.fNewReqQty,>,V.Local.fQtyOnHand)
							F.Intrinsic.Math.Mult(V.DataTable.dtAllShip(V.Local.iCnt).QTY_BO!FieldValFloat,V.Local.fBOMQty,V.Local.fNewBOMQtyReqd)
							F.Intrinsic.String.Build("SELECT * FROM GAB_4192_PARTS WHERE PART = '{0}';",V.Local.sPart,V.Local.sSQL)
							F.ODBC.Connection!con.OpenRecordsetRW("rstParts",V.Local.sSQL)
							F.Intrinsic.Control.If(V.ODBC.con!rstParts.EOF)
								F.ODBC.con!rstParts.AddNew
								F.ODBC.con!rstParts.Set!PART(V.Local.sPart)
								F.ODBC.con!rstParts.Set!LOCATION("")
								F.ODBC.con!rstParts.Set!ORIG_ONHAND(V.Local.fQtyOnHand)
								F.ODBC.con!rstParts.Set!NEW_ONHAND(V.Local.fQtyOnHand)
								F.ODBC.con!rstParts.Update
								F.ODBC.con!rstParts.Requery
							F.Intrinsic.Control.Else
								V.Local.fQtyOnHand.Set(V.ODBC.con!rstParts.FieldValFloat!NEW_ONHAND)
							F.Intrinsic.Control.EndIf
							F.Intrinsic.Control.If(V.Local.fNewBOMQtyReqd,<=,V.Local.fQtyOnHand)
								V.Local.sColor.Set("Y")
								F.Intrinsic.Math.Sub(V.Local.fQtyOnHand,V.Local.fNewBOMQtyReqd,V.Local.fNewOHQty)
								F.ODBC.con!rstParts.Set!NEW_ONHAND(V.Local.fNewOHQty)
								F.ODBC.con!rstParts.Update
								F.ODBC.con!rstParts.Close
							F.Intrinsic.Control.Else(V.Local.fNewBOMQtyReqd,>,V.Local.fQtyOnHand)
								V.Local.sColor.Set("")
								F.ODBC.con!rstParts.Close
								F.Intrinsic.Control.ExitDo
							F.Intrinsic.Control.EndIf
						F.Intrinsic.Control.EndIf
					F.Intrinsic.Control.EndIf
				F.ODBC.con!rstBOM.MoveNext
				F.Intrinsic.Control.Loop
				F.ODBC.con!rstBOM.Close
			F.Intrinsic.Control.EndIf
		F.Intrinsic.Control.EndIf

		F.Intrinsic.Control.If(V.Local.sColor,<>,"")
			F.Data.DataTable.SetValue("dtAllShip",V.Local.iCnt,"COLOR",V.Local.sColor)
		F.Intrinsic.Control.EndIf
	F.Intrinsic.Control.Next(V.Local.iCnt)

	Gui.frmShip.progressBar1.Visible(False)
	
	F.Intrinsic.Control.Catch
		F.Intrinsic.String.Build("Project: {0}{1}{1}Subroutine: {2}{1}Error Occurred {3} with description {4}{1}{1}GAB Version: {5}",V.Ambient.ScriptPath,V.Ambient.Newline,V.Ambient.CurrentSubroutine,V.Ambient.ErrorNumber,V.Ambient.ErrorDescription,V.Ambient.GABVersion,V.Local.sError)
		F.Intrinsic.UI.Msgbox(V.Local.sError)
		F.Intrinsic.Control.End 
	F.Intrinsic.Control.EndTry
Program.Sub.CheckLevel1Reqs.End

